class Employee {
    int id;
    String name;
    double salary;

    // Constructor
    public Employee(int id, String name, double salary) {
        this.id = id;
        this.name = name;
        this.salary = salary;
    }

    // toString() method
    @Override
    public String toString() {
        return id + " - " + name + " - $" + salary;
    }
}
